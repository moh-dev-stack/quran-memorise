{
  "number": 87,
  "name": "Al-A'la",
  "nameArabic": "الأعلى",
  "verses": [
    {
      "number": 1,
      "arabic": "سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَىٰ",
      "transliteration": "Sabbihi isma rabbika al-a'la",
      "translation": "Exalt the name of your Lord, the Most High"
    },
    {
      "number": 2,
      "arabic": "الَّذِي خَلَقَ فَسَوَّىٰ",
      "transliteration": "Alladhi khalaqa fa-sawwa",
      "translation": "Who created and proportioned"
    },
    {
      "number": 3,
      "arabic": "وَالَّذِي قَدَّرَ فَهَدَىٰ",
      "transliteration": "Wa alladhi qaddara fa-hada",
      "translation": "And who destined and [then] guided"
    },
    {
      "number": 4,
      "arabic": "وَالَّذِي أَخْرَجَ الْمَرْعَىٰ",
      "transliteration": "Wa alladhi akhraja al-mar'a",
      "translation": "And who brings out the pasture"
    },
    {
      "number": 5,
      "arabic": "فَجَعَلَهُ غُثَاءً أَحْوَىٰ",
      "transliteration": "Faja'alahu ghutha'an ahwa",
      "translation": "And [then] makes it dark stubble"
    },
    {
      "number": 6,
      "arabic": "سَنُقْرِئُكَ فَلَا تَنسَىٰ",
      "transliteration": "Sanuqri'uka fala tansa",
      "translation": "We will make you recite, [O Muhammad], and you will not forget"
    },
    {
      "number": 7,
      "arabic": "إِلَّا مَا شَاءَ اللَّهُ ۚ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَىٰ",
      "transliteration": "Illa ma sha'a allahu innahu ya'lamu al-jahra wa ma yakhfa",
      "translation": "Except what Allah should will. Indeed, He knows what is declared and what is hidden"
    },
    {
      "number": 8,
      "arabic": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
      "transliteration": "Wa nuyassiruka li-al-yusra",
      "translation": "And We will ease you toward ease"
    },
    {
      "number": 9,
      "arabic": "فَذَكِّرْ إِن نَّفَعَتِ الذِّكْرَىٰ",
      "transliteration": "Fadhakkir in nafa'ati adh-dhikra",
      "translation": "So remind, if the reminder should benefit"
    },
    {
      "number": 10,
      "arabic": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
      "transliteration": "Sayadhakkaru man yakhsha",
      "translation": "He who fears [Allah] will be reminded"
    },
    {
      "number": 11,
      "arabic": "وَيَتَجَنَّبُهَا الْأَشْقَى",
      "transliteration": "Wa yatajannabuha al-ashqa",
      "translation": "But the wretched one will avoid it"
    },
    {
      "number": 12,
      "arabic": "الَّذِي يَصْلَى النَّارَ الْكُبْرَىٰ",
      "transliteration": "Alladhi yasla an-nara al-kubra",
      "translation": "[He] who will [enter and] burn in the greatest Fire"
    },
    {
      "number": 13,
      "arabic": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "transliteration": "Thumma la yamutu fiha wa la yahya",
      "translation": "Neither dying therein nor living"
    },
    {
      "number": 14,
      "arabic": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
      "transliteration": "Qad aflaha man tazakka",
      "translation": "He has succeeded who purifies himself"
    },
    {
      "number": 15,
      "arabic": "وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّىٰ",
      "transliteration": "Wa dhakara isma rabbihi fa-salla",
      "translation": "And mentions the name of his Lord and prays"
    },
    {
      "number": 16,
      "arabic": "بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا",
      "transliteration": "Bal tu'thiruna al-hayata ad-dunya",
      "translation": "But you prefer the worldly life"
    },
    {
      "number": 17,
      "arabic": "وَالْآخِرَةُ خَيْرٌ وَأَبْقَىٰ",
      "transliteration": "Wa al-akhiratu khayrun wa abqa",
      "translation": "While the Hereafter is better and more enduring"
    },
    {
      "number": 18,
      "arabic": "إِنَّ هَٰذَا لَفِي الصُّحُفِ الْأُولَىٰ",
      "transliteration": "Inna hadha lafi as-suhufi al-ula",
      "translation": "Indeed, this is in the former scriptures"
    },
    {
      "number": 19,
      "arabic": "صُحُفِ إِبْرَاهِيمَ وَمُوسَىٰ",
      "transliteration": "Suhufi ibrahima wa musa",
      "translation": "The scriptures of Abraham and Moses"
    }
  ]
}

