{
  "number": 96,
  "name": "Al-Alaq",
  "nameArabic": "العلق",
  "verses": [
    {
      "number": 1,
      "arabic": "اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ",
      "transliteration": "Iqra' bi-ismi rabbika alladhi khalaq",
      "translation": "Recite in the name of your Lord who created"
    },
    {
      "number": 2,
      "arabic": "خَلَقَ الْإِنسَانَ مِنْ عَلَقٍ",
      "transliteration": "Khalaqa al-insana min 'alaq",
      "translation": "Created man from a clinging substance"
    },
    {
      "number": 3,
      "arabic": "اقْرَأْ وَرَبُّكَ الْأَكْرَمُ",
      "transliteration": "Iqra' wa rabbuka al-akram",
      "translation": "Recite, and your Lord is the most Generous"
    },
    {
      "number": 4,
      "arabic": "الَّذِي عَلَّمَ بِالْقَلَمِ",
      "transliteration": "Alladhi 'allama bi-al-qalam",
      "translation": "Who taught by the pen"
    },
    {
      "number": 5,
      "arabic": "عَلَّمَ الْإِنسَانَ مَا لَمْ يَعْلَمْ",
      "transliteration": "'Allama al-insana ma lam ya'lam",
      "translation": "Taught man that which he knew not"
    },
    {
      "number": 6,
      "arabic": "كَلَّا إِنَّ الْإِنسَانَ لَيَطْغَىٰ",
      "transliteration": "Kalla inna al-insana la-yatgha",
      "translation": "No! [But] indeed, man transgresses"
    },
    {
      "number": 7,
      "arabic": "أَن رَّآهُ اسْتَغْنَىٰ",
      "transliteration": "An ra'ahu istaghna",
      "translation": "Because he sees himself self-sufficient"
    },
    {
      "number": 8,
      "arabic": "إِنَّ إِلَىٰ رَبِّكَ الرُّجْعَىٰ",
      "transliteration": "Inna ila rabbika ar-ruj'a",
      "translation": "Indeed, to your Lord is the return"
    },
    {
      "number": 9,
      "arabic": "أَرَأَيْتَ الَّذِي يَنْهَىٰ",
      "transliteration": "Ara'ayta alladhi yanha",
      "translation": "Have you seen the one who forbids"
    },
    {
      "number": 10,
      "arabic": "عَبْدًا إِذَا صَلَّىٰ",
      "transliteration": "'Abdan idha salla",
      "translation": "A servant when he prays?"
    },
    {
      "number": 11,
      "arabic": "أَرَأَيْتَ إِن كَانَ عَلَى الْهُدَىٰ",
      "transliteration": "Ara'ayta in kana 'ala al-huda",
      "translation": "Have you seen if he is upon guidance"
    },
    {
      "number": 12,
      "arabic": "أَوْ أَمَرَ بِالتَّقْوَىٰ",
      "transliteration": "Aw amara bi-at-taqwa",
      "translation": "Or enjoins righteousness?"
    },
    {
      "number": 13,
      "arabic": "أَرَأَيْتَ إِن كَذَّبَ وَتَوَلَّىٰ",
      "transliteration": "Ara'ayta in kaththaba wa tawalla",
      "translation": "Have you seen if he denies and turns away"
    },
    {
      "number": 14,
      "arabic": "أَلَمْ يَعْلَم بِأَنَّ اللَّهَ يَرَىٰ",
      "transliteration": "Alam ya'lam bi-anna allaha yara",
      "translation": "Does he not know that Allah sees?"
    },
    {
      "number": 15,
      "arabic": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ",
      "transliteration": "Kalla la-in lam yantahi la-nasfa'an bi-an-nasiyah",
      "translation": "No! If he does not desist, We will surely drag him by the forelock"
    },
    {
      "number": 16,
      "arabic": "نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ",
      "transliteration": "Nasiyatin kathibah khati'ah",
      "translation": "A lying, sinning forelock"
    },
    {
      "number": 17,
      "arabic": "فَلْيَدْعُ نَادِيَهُ",
      "transliteration": "Falyad'u nadiyah",
      "translation": "Then let him call his associates"
    },
    {
      "number": 18,
      "arabic": "سَنَدْعُ الزَّبَانِيَةَ",
      "transliteration": "Sanad'u az-zabaniyah",
      "translation": "We will call the angels of Hell"
    },
    {
      "number": 19,
      "arabic": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِب ۩",
      "transliteration": "Kalla la tuti'hu wa usjud wa iqtarib",
      "translation": "No! Do not obey him. But prostrate and draw near [to Allah]"
    }
  ]
}

