{
  "number": 86,
  "name": "At-Tariq",
  "nameArabic": "الطارق",
  "verses": [
    {
      "number": 1,
      "arabic": "وَالسَّمَاءِ وَالطَّارِقِ",
      "transliteration": "Wa as-sama'i wa at-tariq",
      "translation": "By the sky and the night comer"
    },
    {
      "number": 2,
      "arabic": "وَمَا أَدْرَاكَ مَا الطَّارِقُ",
      "transliteration": "Wa ma adraka ma at-tariq",
      "translation": "And what can make you know what is the night comer?"
    },
    {
      "number": 3,
      "arabic": "النَّجْمُ الثَّاقِبُ",
      "transliteration": "An-najmu ath-thaqib",
      "translation": "It is the piercing star"
    },
    {
      "number": 4,
      "arabic": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ",
      "transliteration": "In kullu nafsin lamma 'alayha hafiz",
      "translation": "There is no soul but that it has over it a protector"
    },
    {
      "number": 5,
      "arabic": "فَلْيَنظُرِ الْإِنسَانُ مِمَّ خُلِقَ",
      "transliteration": "Falyanzuri al-insanu mimma khuliq",
      "translation": "So let man observe from what he was created"
    },
    {
      "number": 6,
      "arabic": "خُلِقَ مِن مَّاءٍ دَافِقٍ",
      "transliteration": "Khuliq min ma'in dafiq",
      "translation": "He was created from a fluid, ejected"
    },
    {
      "number": 7,
      "arabic": "يَخْرُجُ مِن بَيْنِ الصُّلْبِ وَالتَّرَائِبِ",
      "transliteration": "Yakhruju min bayni as-sulbi wa at-tara'ib",
      "translation": "Emerging from between the backbone and the ribs"
    },
    {
      "number": 8,
      "arabic": "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ",
      "transliteration": "Innahu 'ala raj'ihi laqadir",
      "translation": "Indeed, He [who created] is able to bring him back [to life]"
    },
    {
      "number": 9,
      "arabic": "يَوْمَ تُبْلَىٰ السَّرَائِرُ",
      "transliteration": "Yawma tubla as-sara'ir",
      "translation": "On the Day when secrets will be put on trial"
    },
    {
      "number": 10,
      "arabic": "فَمَا لَهُ مِن قُوَّةٍ وَلَا نَاصِرٍ",
      "transliteration": "Fama lahu min quwwatin wa la nasir",
      "translation": "Then man will have no power or any helper"
    },
    {
      "number": 11,
      "arabic": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ",
      "transliteration": "Wa as-sama'i dhati ar-raj'",
      "translation": "By the sky which returns [rain]"
    },
    {
      "number": 12,
      "arabic": "وَالْأَرْضِ ذَاتِ الصَّدْعِ",
      "transliteration": "Wa al-ardi dhati as-sad'",
      "translation": "And [by] the earth which cracks open"
    },
    {
      "number": 13,
      "arabic": "إِنَّهُ لَقَوْلٌ فَصْلٌ",
      "transliteration": "Innahu laqawlun fasl",
      "translation": "Indeed, it [i.e., the Quran] is a decisive statement"
    },
    {
      "number": 14,
      "arabic": "وَمَا هُوَ بِالْهَزْلِ",
      "transliteration": "Wa ma huwa bi-al-hazl",
      "translation": "And it is not amusement"
    },
    {
      "number": 15,
      "arabic": "إِنَّهُمْ يَكِيدُونَ كَيْدًا",
      "transliteration": "Innahum yakiduna kaydan",
      "translation": "Indeed, they are planning a plan"
    },
    {
      "number": 16,
      "arabic": "وَأَكِيدُ كَيْدًا",
      "transliteration": "Wa akidu kaydan",
      "translation": "But I am planning a plan"
    },
    {
      "number": 17,
      "arabic": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا",
      "transliteration": "Famahhili al-kafirina amhilhum ruwaydan",
      "translation": "So allow time for the disbelievers. Leave them awhile"
    }
  ]
}

